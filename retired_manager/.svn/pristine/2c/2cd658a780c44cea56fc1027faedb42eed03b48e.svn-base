*{
    margin: 0;
    padding: 0;
}
.fl{
    float: left;
}
li{
    list-style: none;
    float: left;
}
@font-face {font-family: 'people';
    src: url('fontface/iconfont.eot'); /* IE9*/
    src: url('fontface/iconfont.eot?#iefix') format('embedded-opentype'), /* IE6-IE8 */
    url('fontface/iconfont.woff') format('woff'), /* chrome、firefox */
    url('fontface/iconfont.ttf') format('truetype')/* chrome、firefox、opera、Safari, Android, iOS 4.2+*/ ; /* iOS 4.1- */
}

.fream{
    width: 1349px;
    /*outline: 1px solid black;*/
    margin: auto;
}
.one-fream{
    width: 1349px;
    height: 50px;
    /*outline: 1px solid red;*/
}
.one-fream .one-a{
    width: 350px;
    height: 50px;
    margin-top: 30px;
    float: left;
    border: 2px solid #787878;
    /*outline: 1px solid black;*/
    text-align: center;
    line-height: 50px;
    margin-left: 100px;
}
.one-fream .one-a>span{
    color: red;
    font-size: 25px;
}
.one-fream .one-b{
    margin-top: 30px;
    width: 750px;
    height: 50px;
    float: left;
    /*outline: 1px solid black;*/
    /*outline: 1px solid #e02222;*/
    text-align: center;
    line-height: 50px;
    margin-left: 100px;
}
.one-blx{
    width: 0px;
    height: 50px;
    /*outline: 1px solid red;*/
    color: white;
    transition: width 2s;
    -moz-transition: width 2s;	/* Firefox 4 */
    -webkit-transition: width 2s;	/* Safari 和 Chrome */
    -o-transition: width 2s;
}
.one-btx{
    width: 0px;
    height: 50px;
    color: white;
    transition: width .6s;
    -moz-transition: width .6s;	/* Firefox 4 */
    -webkit-transition: width .6s;	/* Safari 和 Chrome */
    -o-transition: width .6s;
}
.two-fream{
